schreibeZeile(2 gleich 3 ist). //expect "falsch"
schreibeZeile(2 gleich 2 ist). //expect "wahr"

schreibeZeile(2 ungleich 3 ist). //expect "wahr"
schreibeZeile(2 ungleich 2 ist). //expect "falsch"

schreibeZeile(2 kleiner als 3 ist). //expect "wahr"
schreibeZeile(2 kleiner als 2 ist). //expect "falsch"
schreibeZeile(2 kleiner als 1 ist). //expect "falsch"

schreibeZeile(2 größer als 3 ist). //expect "falsch"
schreibeZeile(2 größer als 2 ist). //expect "falsch"
schreibeZeile(2 größer als 1 ist). //expect "wahr"

schreibeZeile(2 kleiner als, oder 3 ist). //expect "wahr"
schreibeZeile(2 kleiner als, oder 2 ist). //expect "wahr"
schreibeZeile(2 kleiner als, oder 1 ist). //expect "falsch"

schreibeZeile(2 größer als, oder 3 ist). //expect "falsch"
schreibeZeile(2 größer als, oder 2 ist). //expect "wahr"
schreibeZeile(2 größer als, oder 1 ist). //expect "wahr"